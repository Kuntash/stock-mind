{
  "scheme": "stock-mind-scheme",
  "expo": {
    "name": "stock-mind",
    "slug": "stock-mind",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "extra": {
      "eas": {
        "projectId": "720322a0-0669-441e-99de-933851347d8c"
      }
    },
    "owner": "kun_tash",
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
